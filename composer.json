{
  "authors": [
    {
      "name": "Frogg",
      "email": "admin@frogg.fr"
    }
  ],
  "description": "Online Stock Manager",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.0@dev",
    "egulias/email-validator": "^2.0@dev",
    "sensio/framework-extra-bundle": "^5.2@dev",
    "symfony/asset": "dev-master",
    "symfony/browser-kit": "dev-master",
    "symfony/console": "^4.0",
    "symfony/css-selector": "dev-master",
    "symfony/flex": "^1.0",
    "symfony/form": "dev-master",
    "symfony/framework-bundle": "dev-master",
    "symfony/intl": "dev-master",
    "symfony/maker-bundle": "^1.0@dev",
    "symfony/monolog-bundle": "^3.0@dev",
    "symfony/orm-pack": "dev-master",
    "symfony/security-bundle": "dev-master",
    "symfony/swiftmailer-bundle": "^3.2@dev",
    "symfony/translation": "dev-master",
    "symfony/twig-bundle": "dev-master",
    "symfony/validator": "dev-master",
    "symfony/webpack-encore-pack": "dev-master",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "behat/mink": "dev-master",
    "behat/mink-browserkit-driver": "dev-master",
    "behat/mink-extension": "^2.1@dev",
    "behat/mink-goutte-driver": "^1.2@dev",
    "behat/mink-selenium2-driver": "^1.3@dev",
    "behat/symfony2-extension": "^2.1@dev",
    "fzaninotto/faker": "^1.8@dev",
    "sensiolabs/security-checker": "^5.0@dev",
    "symfony/dotenv": "^4.0",
    "symfony/phpunit-bridge": "dev-master",
    "symfony/profiler-pack": "dev-master",
    "symfony/thanks": "^1.0@dev",
    "symfony/web-server-bundle": "dev-master"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "id": "01C9PSWFM6X0FHDDS2112TSXK5",
      "allow-contrib": false
    }
  }
}
